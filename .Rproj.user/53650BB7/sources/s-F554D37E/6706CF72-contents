
library(dplyr)
library(readr)
CarrerSalsryByIndustry103<-read_csv("C:/Users/hsuen/Downloads/SalaryTable/103CarrerSalsryByIndustry.csv")
CarrerSalsryByIndustry106<-read_csv("C:/Users/hsuen/Downloads/SalaryTable/106CarrerSalsryByIndustry.csv")

CarrerSalsryByIndustry103<-rename(CarrerSalsryByIndustry103,Salary=`經常性薪資-薪資`)
CarrerSalsryByIndustry106<-rename(CarrerSalsryByIndustry106,Salary=`經常性薪資-薪資`)
CarrerSalsryByIndustry103$Salary<-as.numeric(CarrerSalsryByIndustry103$Salary)
CarrerSalsryByIndustry106$Salary<-as.numeric(CarrerSalsryByIndustry106$Salary)

S106BS103<-filter(CarrerSalsryByIndustry106,CarrerSalsryByIndustry106$Salary>CarrerSalsryByIndustry103$Salary)
